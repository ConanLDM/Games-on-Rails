<%= form_with model: @review do |f| %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :body %>
    <%= f.text_area :body %>
    <%= f.label :content %>
    <%= f.rich_text_area :content %>
    <%= f.label :rating %>
    <%= f.number_field :rating %>
    <%= f.submit "Create" %>
  </div>
<% end %>
